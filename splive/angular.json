{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "spl": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/spl",
            "index": "src/app/spl/index.html",
            "main": "src/app/spl/main.ts",
            "tsConfig": "src/app/spl/tsconfig.spl.json",
            "polyfills": "src/app/spl/polyfills.ts",
            "assets": [
              {
                "glob": "**/*",
                "input": "src/app/spl/assets",
                "output": "/assets"
              },
              {
                "glob": "favicon.ico",
                "input": "src/app/spl/spl",
                "output": "/"
              },
              {
                "glob": "**/*",
                "input": "src/app/shared/assets",
                "output": "/shared/assets"
              }
            ],
            "styles": [
              "src/app/spl/styles.css",
              "src/app/shared/shared-styles.css",
              "node_modules/ngx-toastr/toastr.css",
              "src/app/shared/theme.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/app/spl/environments/environment.ts",
                  "with": "src/app/spl/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "spl:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "spl:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "spl:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/app/spl/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/app/spl/polyfills.ts",
            "tsConfig": "src/app/spl/tsconfig.spec.json",
            "scripts": [],
            "styles": [
              "src/app/spl/styles.css",
              "src/app/shared/shared-styles.css",
              "node_modules/ngx-toastr/toastr.css",
              "src/app/shared/theme.scss"
            ],
            "assets": [
              {
                "glob": "**/*",
                "input": "src/app/spl/assets",
                "output": "/assets"
              },
              {
                "glob": "favicon.ico",
                "input": "src/app/spl/spl",
                "output": "/"
              },
              {
                "glob": "**/*",
                "input": "src/app/shared/assets",
                "output": "/shared/assets"
              }
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "spl-e2e": {
      "root": "",
      "sourceRoot": "",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "spl:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "e2e/tsconfig.e2e.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "hkt": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/hkt",
            "index": "src/app/hkt/index.html",
            "main": "src/app/hkt/main.ts",
            "tsConfig": "src/app/hkt/tsconfig.hkt.json",
            "polyfills": "src/app/hkt/polyfills.ts",
            "assets": [
              {
                "glob": "**/*",
                "input": "src/app/hkt/assets",
                "output": "/assets"
              },
              {
                "glob": "favicon.ico",
                "input": "src/app/hkt/hkt",
                "output": "/"
              },
              {
                "glob": "**/*",
                "input": "src/app/shared/assets",
                "output": "/shared/assets"
              }
            ],
            "styles": [
              "src/app/hkt/styles.css",
              "src/app/shared/shared-styles.css",
              "node_modules/ngx-toastr/toastr.css",
              "node_modules/dragula/dist/dragula.min.css",
              "src/app/shared/theme.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/app/hkt/environments/environment.ts",
                  "with": "src/app/hkt/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "hkt:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "hkt:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "hkt:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/app/hkt/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/app/hkt/polyfills.ts",
            "tsConfig": "src/app/hkt/tsconfig.spec.json",
            "scripts": [],
            "styles": [
              "src/app/hkt/styles.css",
              "src/app/shared/shared-styles.css",
              "node_modules/ngx-toastr/toastr.css",
              "node_modules/dragula/dist/dragula.min.css",
              "src/app/shared/theme.scss"
            ],
            "assets": [
              {
                "glob": "**/*",
                "input": "src/app/hkt/assets",
                "output": "/assets"
              },
              {
                "glob": "favicon.ico",
                "input": "src/app/hkt/hkt",
                "output": "/"
              },
              {
                "glob": "**/*",
                "input": "src/app/shared/assets",
                "output": "/shared/assets"
              }
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "hkt-e2e": {
      "root": "",
      "sourceRoot": "",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "hkt:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "e2e/tsconfig.e2e.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "spl",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "app",
      "styleext": "css"
    },
    "@schematics/angular:directive": {
      "prefix": "app"
    }
  }
}