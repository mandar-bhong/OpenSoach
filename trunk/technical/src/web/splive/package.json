    "buildlocal:spl": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build spl --prod",
    "buildlocal:hkt": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build hkt --prod",
